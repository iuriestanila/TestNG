<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="JV">
    <test name = "Exclude/Include methods from classes">

        <classes>

            <class name="TestNGTests.CartTest">
                <methods>
                    <exclude name="totalPriceRealItem"/>
                </methods>
            </class>

            <class name="TestNGTests.JsonParserTest">
                <methods>
                    <include name="FileNotFoundExceptionFileWriter"/>
                </methods>
            </class>

        </classes>

    </test>
</suite>